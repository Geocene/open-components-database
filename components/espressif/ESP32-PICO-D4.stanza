    [CLK         | 31           | Right   ]
    [CMD         | 30           | Right   ]
    [SD[0]       | 32           | Right   ]
    [SD[1]       | 33           | Right   ]
    [SD[2]       | 28           | Right   ]
    [SD[3]       | 29           | Right   ]
    [IO[0]       | 23           | Left    ]
    [IO[2]       | 22           | Right   ]
    [IO[4]       | 24           | Right   ]
    [IO[5]       | 34           | Right   ]
    [IO[12]      | 18           | Right   ]
    [IO[13]      | 20           | Right   ]
    [IO[14]      | 17           | Right   ]
    [IO[15]      | 21           | Right   ]
    [IO[16]      | 25           | Right   ]
    [IO[17]      | 27           | Left    ]
    [IO[18]      | 35           | Left    ]
    [IO[19]      | 38           | Left    ]
