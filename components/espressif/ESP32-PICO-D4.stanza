    [IO[22]      | 39           | Left    ]
    [IO[23]      | 36           | Left    ]
    [IO[25]      | 14           | Left    ]
    [IO[26]      | 15           | Left    ]
    [IO[27]      | 16           | Left    ]
    [IO[32]      | 12           | Left    ]
    [IO[33]      | 13           | Left    ]
    [IO[34]      | 10           | Left    ]
    [IO[35]      | 11           | Left    ]
    [U0RXD       | 40           | Left    ]
    [U0TXD       | 41           | Left    ]
    [LNA-IN      | 2            | Right   ]
    [GND         | 49           | Down    ]
    [VDDA        | 1, 43, 46    | Up      ]
    [VDDA3P3     | 3, 4         | Up      ]
    [VDD-SDIO    | 26           | Up      ]
    [VDD3P3-CPU  | 37           | Up      ]
    [VDD3P3-RTC  | 19           | Up      ]
    [SENSOR-VN   | 8            | Left    ]
    [SENSOR-VP   | 5            | Left    ]
    [SENSOR-CAPN | 7            | Left    ]
    [SENSOR-CAPP | 6            | Left    ]
    [nc[0]       | 44           | Down    ]
    [nc[1]       | 45           | Down    ]
    [nc[2]       | 47           | Down    ]
    [nc[3]       | 48           | Down    ]

  make-box-symbol()
  assign-package(pqfn50p700x700x104-49n)

  ; ESP supports crossbarred IO, so use generic supports
  val reserved-io = [IO[0] IO[16] IO[17] IO[32] IO[33]]
  for IO-pin in ports(IO) do :
    if not contains?(reserved-io, IO-pin) :
      supports io-pin:
        io-pin.p => IO-pin

; Generic digital pin
pcb-bundle io-pin:
  public pin p


public pcb-module module :
  public port uart : uart-with([`dtr `rts])
  public port power : power
  public pin ant
  public pin gnd
  public pin en
  public pin boot
  public inst mcu : ocdb/espressif/ESP32-PICO-D4/component

  net (gnd, mcu.GND, power.gnd)
  net (power.vdd, mcu.VDDA, mcu.VDDA3P3, mcu.VDD3P3-CPU, mcu.VDD3P3-RTC)
  bypass-cap-strap(mcu.VDDA3P3, gnd, 10.0e-6)
  bypass-cap-strap(mcu.VDDA3P3, gnd, 0.1e-6)
  net (mcu.LNA-IN, ant)

  ; UART programming interface using control pins
  net ESP-RX (mcu.U0RXD, uart.rx)
  net ESP-TX (mcu.U0TXD, uart.tx)
  public inst q : ocdb/nexperia/PMBT3904/component[2]
  res-strap(uart.dtr, q[0].b, 10.0e3)
  res-strap(uart.rts, q[1].b, 10.0e3)
  net BOOT (mcu.IO[0], q[1].c boot)
  net (mcu.EN, q[0].c en)
  net (uart.rts, q[0].e)
  net (uart.dtr, q[1].e)

  ; Supports statements for digital peripherals
  for i in 0 to 22 do:
    supports gpio:
      require pin:io-pin from mcu
      gpio.gpio => io-pin.p
  for i in 0 to 16 do:
    supports pwm:
      require pin:io-pin from mcu
      pwm.pwm => io-pin.p
  for i in 0 to 2 do:
    supports i2c:
      require pin:io-pin[2] from mcu
      i2c.sda => pin[0].p
      i2c.scl => pin[1].p
  for i in 0 to 2 do:
    supports ocdb/bundles/uart:
      require pin:io-pin[2] from mcu
      uart.tx => pin[0].p
      uart.rx => pin[1].p





