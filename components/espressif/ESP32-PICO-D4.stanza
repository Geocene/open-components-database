  public pin VDD3P3-RTC 
  public pin SENSOR-VN 
  public pin SENSOR-VP 
  public pin SENSOR-CAPN 
  public pin SENSOR-CAPP 
  public port nc : pin[4]

  pin-properties :
    [pin:Ref     | pads:Int ... | side:Dir]
